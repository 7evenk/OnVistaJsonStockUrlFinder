{
    "manifest_version": 3,
    "name": "OnVistaJsonStockUrlFinderForPortfolioPerformance",
    "version": "1.0",
    "permissions": ["webRequest", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "OnVistaJsonStockUrlFinderForPortfolioPerformance"
    },
    "content_scripts": [
      {
        "matches": ["*://*.onvista.de/*"],
        "js": ["content.js"]
      }
    ]
  }
  